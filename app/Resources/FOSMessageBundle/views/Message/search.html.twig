{% extends 'FOSMessageBundle::layout.html.twig' %}

{% block fos_message_content %}

<div class="asphalt-bg half-padded half-gap-bottom">
    <i class="icon-search gap-right gap-left"></i>{% trans from 'FOSMessageBundle' %}search{% endtrans %}
</div>

<p>{% transchoice threads|length with {'%num%': threads|length } from 'FOSMessageBundle' %}
